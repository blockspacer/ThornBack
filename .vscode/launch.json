	"version": "0.2.0",
	"configurations": [
		{
			"name": "(Windows) Launch",
			"type": "cppvsdbg",
			"request": "launch",
			"program": "${workspaceFolder}\\..\\godot\\bin\\godot.windows.tools.64.exe",
			"args": [
				"-v",
				"-d"
			],
			"stopAtEntry": false,
			"cwd": "${workspaceFolder}\\Project",
			"environment": [],
			"externalConsole": false,
			"preLaunchTask": "build",
			"internalConsoleOptions": "openOnSessionStart"
		},
		{
			"name": "Launch Editor",
			"type": "cppvsdbg",
			"request": "launch",
			"program": "${workspaceFolder}\\..\\godot\\bin\\godot.windows.tools.64.exe",
			"args": [
				"-v",
				"-d",
				"-e"
			],
			"stopAtEntry": false,
			"cwd": "${workspaceFolder}\\Project",
			"environment": [],
			"externalConsole": false,
			"preLaunchTask": "build",
			"internalConsoleOptions": "openOnSessionStart"
		}
	]
}